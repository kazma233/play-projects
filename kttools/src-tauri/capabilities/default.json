{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    "dialog:default",
    "fs:default",
    "fs:allow-write-file",
    "clipboard-manager:default",
    "clipboard-manager:allow-clear",
    "clipboard-manager:allow-write-image"
  ]
}